    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch (prod db)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/HackathonWebApp/bin/Debug/netcoreapp3.1/HackathonWebApp.dll",
            "args": [],
            "cwd": "${workspaceFolder}/HackathonWebApp",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_URLS": "https://localhost:5001",
                "ASPNETCORE_ENVIRONMENT": "Production"
            },
            "envFile": "${workspaceFolder}/.env.prod"
        },
        {
            "name": "Launch (dev db)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/HackathonWebApp/bin/Debug/netcoreapp3.1/HackathonWebApp.dll",
            "args": [],
            "cwd": "${workspaceFolder}/HackathonWebApp",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_URLS": "https://localhost:5001",
                "ASPNETCORE_ENVIRONMENT": "Development",
            },
            "envFile": "${workspaceFolder}/.env.develop"
        }
    ]
}